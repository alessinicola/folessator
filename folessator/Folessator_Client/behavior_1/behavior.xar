                                    <![CDATA[import socket
import sys
import struct

class MySocket:
    sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    server_address = ('localhost', 5829)

    @classmethod
    def open(arg):
        print >>sys.stderr, 'connecting to %s port %s' % MySocket.server_address
        MySocket.sock.connect(MySocket.server_address)

    @classmethod
    def close(arg):
        MySocket.sock.close()

    @classmethod
    def recvall(arg,size):
        received_chunks = []
        buf_size = 4096
        remaining = size
        while remaining > 0:
            received = MySocket.sock.recv(min(remaining, buf_size))
            if not received:
                raise Exception('unexcepted EOF')
            received_chunks.append(received)
            remaining -= len(received)
        return b''.join(received_chunks)

    @classmethod
    def write_utf8(arg,s):
        encoded = s.encode(encoding='utf-8')
        MySocket.sock.sendall(struct.pack('>i', len(encoded)))
        MySocket.sock.sendall(encoded)

    @classmethod
    def read_utf8(arg):
        len_bytes = MySocket.recvall( 4)
        length = struct.unpack('>i', len_bytes)[0]
        encoded = MySocket.recvall(length)
       # return str(encoded, encoding='utf-8')
        return str(encoded.encode('utf-8'))

    @classmethod
    def read_bool(arg):
        data = MySocket.sock.recv(1)
        if (data == ""):
            return 1
        else:
            return 0]]>
